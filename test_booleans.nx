// Testing boolean operations
fn main() -> Int {
    let is_true: Bool = true;
    let is_false: Bool = false;
    
    println("Testing boolean values:");
    
    if is_true {
        println("Boolean true works");
    }
    
    if is_false {
        println("This should not print");
    } else {
        println("Boolean false works");
    }
    
    // Test boolean logic
    let a: Int = 5;
    let b: Int = 10;
    
    if a < b {
        println("Comparison works: 5 < 10");
    }
    
    if a > b {
        println("This should not print");
    } else {
        println("Else condition works");
    }
    
    return 0;
}
